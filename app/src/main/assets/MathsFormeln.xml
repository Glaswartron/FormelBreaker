        <LaTeX lang="DE">$$O = 2 \cdot \pi \cdot r^2 + M$$</LaTeX>
        <LaTeX lang="DE">$$M = O - 2 \cdot \pi \cdot r^2 $$</LaTeX>
        <LaTeX lang="DE">$$r = \sqrt{\frac{O - M}{2 \cdot \pi}}$$</LaTeX>
        <Variable>surface_zylinder</Variable>
        <Variable>zylindermantel</Variable>
        <Variable>radius_maths</Variable>
    </Formel>

    <Formel>
        <Category>surface</Category>
        <ButtonText lang="DE">Kegelmantelfläche</ButtonText>
        <ButtonText lang="ENG">Lateral surface area of a cone</ButtonText>
        <Preview lang="DE">M = r · s · π</Preview>
        <Umformung>r * s * pi</Umformung>
        <Umformung>M / (s * pi)</Umformung>
        <Umformung>M / (r * pi)</Umformung>
        <LaTeX lang="DE">$$M = r \cdot s \cdot \pi$$</LaTeX>
        <LaTeX lang="DE">$$r = \frac{M}{s \cdot \pi}$$</LaTeX>
