    <Formel>
        <Category>func</Category>
        <ButtonText lang="DE">Sinus</ButtonText>
        <ButtonText lang="ENG">Sine</ButtonText>
        <Preview lang="DE">sin(α) = Gegenkathete ÷ Hypotenuse</Preview>
        <Umformung>G / H</Umformung>
        <Umformung>s * H</Umformung>
        <Umformung>s / G</Umformung>
        <LaTeX lang="DE">$$\sin{\alpha} = \frac{G}{H}$$</LaTeX>
        <LaTeX lang="DE">$$G = \sin{\alpha} \cdot H$$</LaTeX>
        <LaTeX lang="DE">$$H = \frac{\sin{\alpha}}{G}$$</LaTeX>
        <Variable>sinus_alpha</Variable>
        <Variable>gegenkathete</Variable>
        <Variable>hypotenuse</Variable>
    </Formel>

    <Formel>
        <Category>func</Category>
        <ButtonText lang="DE">Cosinus</ButtonText>
        <ButtonText lang="ENG">Cosine</ButtonText>
        <Preview lang="DE">cos(α) = Ankathete ÷ Hypotenuse</Preview>
        <Umformung>A / H</Umformung>
        <Umformung>c * H</Umformung>
        <Umformung>c / A</Umformung>
        <LaTeX lang="DE">$$\cos{\alpha} = \frac{A}{H}$$</LaTeX>
        <LaTeX lang="DE">$$A = \cos{\alpha} \cdot H$$</LaTeX>
        <LaTeX lang="DE">$$H = \frac{\cos{\alpha}}{A}$$</LaTeX>
        <Variable>cosinus_alpha</Variable>
        <Variable>ankathete</Variable>
        <Variable>hypotenuse</Variable>
    </Formel>

    <Formel>
        <Category>func</Category>
        <ButtonText lang="DE">Tangens</ButtonText>
        <ButtonText lang="ENG">Tangent</ButtonText>
        <Preview lang="DE">tan(α) = Gegenkathete ÷ Ankathete</Preview>
        <Umformung>G / A</Umformung>
        <Umformung>t * A</Umformung>
        <Umformung>t / G</Umformung>
        <LaTeX lang="DE">$$\tan{\alpha} = \frac{G}{A}$$</LaTeX>
        <LaTeX lang="DE">$$G = \tan{\alpha} \cdot A$$</LaTeX>
        <LaTeX lang="DE">$$A = \frac{\tan{\alpha}}{H}$$</LaTeX>
        <Variable>tangens_alpha</Variable>
        <Variable>gegenkathete</Variable>
        <Variable>ankathete</Variable>
    </Formel>

    <Formel>
        <Category>func</Category>
        <ButtonText lang="DE">Cotangens</ButtonText>
        <ButtonText lang="ENG">Cotangent</ButtonText>
        <Preview lang="DE">cot(α) = Ankathete ÷ Gegenkathete</Preview>
        <Umformung>A / G</Umformung>
        <Umformung>a * G</Umformung>
        <Umformung>a / A</Umformung>
        <LaTeX lang="DE">$$\cot{\alpha} = \frac{A}{G}$$</LaTeX>
        <LaTeX lang="DE">$$A = \cot{\alpha} \cdot G$$</LaTeX>
        <LaTeX lang="DE">$$G = \frac{\cot{\alpha}}{A}$$</LaTeX>
        <Variable>cotangens_alpha</Variable>
        <Variable>ankathete</Variable>
        <Variable>gegenkathete</Variable>
    </Formel>

    <Formel>
        <Category>func</Category>
        <ButtonText lang="DE">Tangens aus Sinus und Cosinus</ButtonText>
        <ButtonText lang="ENG">Tangent from Sine and Cosine</ButtonText>
        <Preview lang="DE">tan(α) = sin(α) ÷ cos(α)</Preview>
        <Umformung>s / c</Umformung>
        <Umformung>t * c</Umformung>
        <Umformung>t / s</Umformung>
        <LaTeX lang="DE">$$\tan{\alpha} = \frac{\sin{\alpha}}{\cos{\alpha}}$$</LaTeX>
        <LaTeX lang="DE">$$sin{\alpha} = \tan{\alpha} \cdot \cos{\alpha}$$</LaTeX>
        <LaTeX lang="DE">$$\cos{\alpha} = \frac{\tan{\alpha}}{\sin{\alpha}}$$</LaTeX>
        <Variable>tangens_alpha</Variable>
        <Variable>sinus_alpha</Variable>
        <Variable>cosinus_alpha</Variable>
    </Formel>

    <Formel>
        <Category>umf</Category>
        <ButtonText lang="DE">Rechtecksumfang</ButtonText>
        <ButtonText lang="ENG">Circumference of a rectangle</ButtonText>
        <Preview lang="DE">U = 2a + 2b</Preview>
        <Umformung>(2 * a) + (2 * b)</Umformung>
        <Umformung>(U – (2 * b)) / 2</Umformung>
        <Umformung>(U – (2 * b)) / 2</Umformung>
        <LaTeX lang="DE">$$U = 2a + 2b$$</LaTeX>
        <LaTeX lang="DE">$$a = \frac{U – 2b}{2}$$</LaTeX>
        <LaTeX lang="DE">$$b = \frac{U – 2a}{2}$$</LaTeX>
        <Variable>umfang_rechteck</Variable>
        <Variable>length</Variable>
        <Variable>width</Variable>
    </Formel>

    <Formel>
        <Category>umf</Category>
        <ButtonText lang="DE">Umfang eines gleichseitigen n-Ecks</ButtonText>
        <ButtonText lang="ENG">Circumference of a equilateral n-gon</ButtonText>
        <Preview lang="DE">U = n ⋅ a</Preview>
        <Umformung>n * a</Umformung>
        <Umformung>U / a</Umformung>
        <Umformung>U / n</Umformung>
        <LaTeX lang="DE">$$U = n \cdot a$$</LaTeX>
        <LaTeX lang="DE">$$n = \frac{U}{a}$$</LaTeX>
        <LaTeX lang="DE">$$a = \frac{U}{n}$$</LaTeX>
        <Variable>umfang_neck</Variable>
        <Variable>anzahl_seiten</Variable>
        <Variable>seitenlength</Variable>
    </Formel>
    <Formel>
        <Category>lengths</Category>
        <ButtonText lang="DE">Diagonale im Rechteck</ButtonText>
        <ButtonText lang="ENG">Diagonal in a rectangle</ButtonText>
        <Preview lang="DE">d = √ (a² + b²)</Preview>
        <Umformung>sqrt((a^2) + (b^2))</Umformung>
        <Umformung>sqrt((d^2) - (b^2))</Umformung>
        <Umformung>sqrt((d^2) - (a^2))</Umformung>
        <LaTeX lang="DE">$$d = \sqrt{a^2 + b^2}$$</LaTeX>
        <LaTeX lang="DE">$$a = \sqrt{d^2 - b^2}$$</LaTeX>
        <LaTeX lang="DE">$$b = \sqrt{d^2 - a^2}$$</LaTeX>
        <Variable>diagonale</Variable>
        <Variable>length</Variable>
        <Variable>width</Variable>
    </Formel>

    <Formel>
        <Category>lengths</Category>
        <ButtonText lang="DE">Raumdiagonale eines Körpers</ButtonText>
        <ButtonText lang="ENG">Space diagonal of a body</ButtonText>
        <Preview lang="DE">dᵣ = √(d² + h²)</Preview>
        <Umformung>sqrt((d^2) + (h^2))</Umformung>
        <Umformung>sqrt((dr^2) - (h^2))</Umformung>
        <Umformung>sqrt((dr^2) - (d^2))</Umformung>
        <LaTeX lang="DE">$$d_R = \sqrt{d^2 + h^2}$$</LaTeX>
        <LaTeX lang="DE">$$d = \sqrt{{d_R}^2 - h^2}$$</LaTeX>
        <LaTeX lang="DE">$$h = \sqrt{{d_R}^2 - d^2}$$</LaTeX>
        <Variable>raumdiagonale</Variable>
        <Variable>flaechendiagonale</Variable>
        <Variable>height_maths</Variable>
    </Formel>

    <Formel>
        <Category>lengths</Category>
        <ButtonText lang="DE">Höhe eines gleichseitigen Dreiecks</ButtonText>
        <ButtonText lang="ENG">Height of an equilateral triangle</ButtonText>
        <Preview lang="DE">h = √3 ⋅ (0,5 ⋅ a)</Preview>
        <Umformung>(a / 2) * sqrt(3)</Umformung>
        <Umformung>(2 * h) / sqrt(3)</Umformung>
        <LaTeX lang="DE">$$h = \frac{a}{2} \cdot \sqrt{3}$$</LaTeX>
        <LaTeX lang="DE">$$a = \frac{2 \cdot h}{\sqrt{3}}$$</LaTeX>
        <Variable>height_maths</Variable>
        <Variable>seitenlength</Variable>
    </Formel>

</Formeln>